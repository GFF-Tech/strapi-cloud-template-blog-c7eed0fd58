{
  "kind": "collectionType",
  "collectionName": "speakers",
  "info": {
    "singularName": "speaker",
    "pluralName": "speakers",
    "displayName": "speaker",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "salutation": {
      "type": "enumeration",
      "enum": [
        "Shri",
        "Mr.",
        "Ms.",
        "Mrs",
        "Dr.",
        "Smt.",
        "Lt"
      ],
      "required": true
    },
    "fullName": {
      "type": "string",
      "required": true
    },
    "desgination": {
      "type": "string"
    },
    "companyName": {
      "type": "string"
    },
    "country": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::country.country"
    },
    "bio": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "linkedinProfile": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "speakerId": {
      "type": "uid"
    },
    "featureinHomePage": {
      "type": "boolean"
    },
    "speakerPriorityinSpeakerPage": {
      "type": "decimal"
    },
    "speakerPriorityinHomePage": {
      "type": "decimal"
    },
    "speakerType": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "One - Tier 1",
        "Two - Category A",
        "Three - Category B",
        "Four - Category C"
      ]
    },
    "agenda_dd": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::agenda-dd.agenda-dd",
      "inversedBy": "session_speakers"
    }
  }
}
